<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
          http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="student" class="com.stackroute.DI.Student">
        <property name="name" value="John"></property>
        <property name="studentId" value="1"></property>
        <property name="age" value="16"></property>
        <property name="gen" value="Male"></property>
    </bean>

    <bean id="student2" class="com.stackroute.DI.Student">
        <property name="name" value="Jane"></property>
        <property name="studentId" value="2"></property>
        <property name="age" value="18"></property>
        <property name="gen" value="Female"></property>
    </bean>





</beans>



